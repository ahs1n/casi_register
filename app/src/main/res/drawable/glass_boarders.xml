<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-2dp"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="#11111111" />
            <stroke
                android:width="4dp"
                android:color="#fff" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="-45"
                android:centerColor="#55d6d6d6"
                android:endColor="#5520344b"
                android:startColor="#5520bfD6" />
            <corners android:radius="2dp" />

            <stroke
                android:width="4dp"
                android:color="#ff207d94" />
        </shape>
    </item>


</layer-list>
